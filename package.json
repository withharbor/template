  "devDependencies": {
    "@types/ioredis": "^4.22.0",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@types/node-schedule": "^1.3.1",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "prettier": "^2.2.1",
    "prisma": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@prisma/client": "^3.0.2",
    "@sentry/browser": "^6.11.0",
    "@sentry/node": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "@types/datadog-metrics": "^0.6.2",
    "@types/express": "^4.17.13",
    "@types/spotify-web-api-node": "^5.0.2",
    "axios": "^0.21.4",
    "cheerio": "^1.0.0-rc.10",
    "datadog-metrics": "^0.9.3",
    "dayjs": "^1.10.7",
    "discord-reply": "^0.1.2",
    "discord.js": "^13.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ioredis": "^4.24.2",
    "node-fetch": "^2.6.1",
    "node-schedule": "^2.0.0",
    "permer": "^1.0.7",
    "signale": "^1.4.0",
    "spotify-web-api-node": "^5.0.2",
    "topgg-autoposter": "^2.0.0",
    "zod": "^3.0.0-beta.1"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --exit-child src/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node .",
    "generate": "prisma generate",
    "lint": "eslint .",
    "services:up": "docker-compose -f docker-compose.dev.yml up -d&&yarn migrate",
    "services:down": "docker-compose -f docker-compose.dev.yml down",
    "services:start": "docker-compose -f docker-compose.dev.yml start",
    "services:stop": "docker-compose -f docker-compose.dev.yml stop",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "up:prod": "docker-compose up --build -d",
    "down:prod": "docker-compose down",
    "prod": "yarn migrate:deploy && yarn start"
  }
}
